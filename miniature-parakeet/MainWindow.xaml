<Window x:Class="miniature_parakeet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:miniature_parakeet.UserControls"
        mc:Ignorable="d"
        Title="Miniature Parakeet" Height="450" Width="800">

    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_File"></MenuItem>
            <MenuItem Header="_Edit"></MenuItem>
            <MenuItem Header="_Help"></MenuItem>
        </Menu>

        <uc:UpcomingEvents DockPanel.Dock="Right" Margin="5" />
        <uc:MonthPager DockPanel.Dock="Bottom" />
        <uc:MonthList />
    </DockPanel>
</Window>
